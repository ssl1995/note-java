<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <context id="Mysql" targetRuntime="MyBatis3" defaultModelType="flat">

        <!-- 自动检查关键字，为关键字增加反引号 -->
        <property name="autoDelimitKeywords" value="true"/>
        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>

        <!--覆盖生成XML文件-->
        <plugin type="org.mybatis.generator.plugins.UnmergeableXmlMappersPlugin"/>
        <!-- 生成的实体类添加toString()方法 -->
        <plugin type="org.mybatis.generator.plugins.ToStringPlugin"/>

        <!-- 不生成注释 -->
        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://rm-2zep5730lmk5s5dy37o.mysql.rds.aliyuncs.com:3306/mall-learn?characterEncoding=UTF8&amp;autoReconnect=true&amp;serverTimezone=Asia/Shanghai&amp;allowMultiQueries=true"
                        userId="root"
                        password="Ssl@199502">
        </jdbcConnection>

        <!-- domain/model类的位置 -->
        <javaModelGenerator targetProject="note-mall/src/main/java"
                            targetPackage="com.ssl.mall.notemall.mbg.model"/>

        <!-- mapper.xml的位置 -->
        <sqlMapGenerator targetProject="note-mall/src/main/resources"
                         targetPackage="com.ssl.mall.notemall.mbg.mapper"/>

        <!-- mapper接口的位置 -->
        <javaClientGenerator targetProject="note-mall/src/main/java"
                             targetPackage="com.ssl.mall.notemall.mbg.mapper"
                             type="XMLMAPPER"/>

        <!-- 选择生成的表-->
        <table tableName="pms_brand">
            <generatedKey column="id" sqlStatement="MySql" identity="true"/>
        </table>
    </context>

